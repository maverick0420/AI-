REACT_APP_API_URL=https://api.aty.live
REACT_APP_WS_URL=wss://api.aty.live
REACT_APP_MEDIA_URL=https://cdn.aty.live

# 服务器配置
PORT=3001
NODE_ENV=development

# 数据库配置
MONGODB_URI=mongodb://localhost:27017/aty_live

# JWT配置
JWT_SECRET=your_jwt_secret_key
